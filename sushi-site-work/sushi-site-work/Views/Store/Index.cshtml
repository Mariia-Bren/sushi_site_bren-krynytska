@model IEnumerable<sushi_site_work.Models.SubCategory>
@{
    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>
<p>
    Select from @Model.Count()
    genres:
</p>
<ul>
    @foreach (var subCategory in Model)
    {
        <li>
            @Html.ActionLink(subCategory.Name, "Browse", new { subCategory = subCategory.Name })
        </li>
    }
</ul>

